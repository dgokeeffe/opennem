{
    "$schema": "https://mintlify.com/schema.json",
    "theme": "prism",
    "name": "OpenElectricity Documentation",
    "openapi": "https://api.openelectricity.org.au/v4/openapi.json",
    "logo": {
        "dark": "/images/oe_logo_light.png",
        "light": "/images/oe_logo_light.png"
    },
    "favicon": "/images/favicon/favicon.svg",
    "colors": {
        "primary": "#8b3119",
        "light": "#9b3f23",
        "dark": "#7b2315",
        "anchors": {
            "from": "#A29D66",
            "to": "#928D56"
        }
    },
    "feedback": {
        "suggestEdit": true,
        "raiseIssue": true,
        "thumbsRating": true
    },
    "font": {
        "headings": {
            "family": "DM Sans, sans-serif",
            "weight": 400
        },
        "body": {
            "family": "DM Sans, sans-serif",
            "weight": 400
        }
    },
    "api": {
        "baseUrl": "https://api.openelectricity.org.au/v4",
        "auth": {
            "method": "bearer"
        }
    },
    "topbarLinks": [
        {
            "name": "Tracker",
            "url": "https://explore.openelectricity.org.au"
        },
        {
            "name": "GitHub",
            "url": "https://github.com/opennem/opennem"
        },
        {
            "name": "Blog",
            "url": "https://openelectricity.org.au/analysis"
        }
    ],
    "topbarCtaButton": {
        "name": "Platform",
        "url": "https://platform.openelectricity.org.au"
    },
    "tabs": [
        {
            "name": "API Reference",
            "url": "api"
        },
        {
            "name": "SDKs",
            "url": "sdk"
        },
        {
            "name": "Contribute",
            "url": "contribute"
        }
    ],
    "primaryTab": {
        "name": "Documentation"
    },
    "anchors": [
        {
            "name": "GitHub",
            "icon": "github",
            "url": "https://github.com/opennem"
        },
        {
            "name": "Website",
            "icon": "link",
            "url": "https://explore.openelectricity.org.au/"
        }
    ],
    "navigation": [
        {
            "group": "Get Started",
            "pages": ["introduction"]
        },
        {
            "group": "Guides",
            "pages": [
                "guides/networks",
                "guides/power",
                "guides/energy",
                "guides/price",
                "guides/emissions",
                "guides/fueltechs",
                "guides/facilities",
                "guides/batteries",
                "guides/weather",
                "guides/interconnectors"
            ]
        },
        {
            "group": "Platform",
            "pages": ["platform/introduction"]
        },
        {
            "group": "API Reference",
            "pages": [
                "api/overview",
                {
                    "group": "Generation Data",
                    "pages": [
                        "api/data/get-network-data",
                        "api/data/get-facility-data"
                    ]
                },
                {
                    "group": "Market Data",
                    "pages": ["api/market/get-network-data"]
                },
                {
                    "group": "Facilities",
                    "pages": ["api/facilities/get-facilities"]
                },
                {
                    "group": "User",
                    "pages": ["api/user/get-user-me"]
                }
            ]
        },
        {
            "group": "SDKs",
            "pages": [
                "sdk/overview",
                "sdk/configuration",
                {
                    "group": "Typescript",
                    "pages": [
                        "sdk/typescript/overview",
                        "sdk/typescript/reference",
                        "sdk/typescript/types",
                        "sdk/typescript/utilities"
                    ]
                },
                {
                    "group": "Python",
                    "pages": ["sdk/python/overview"]
                }
            ]
        },
        {
            "group": "Contribute",
            "pages": [
                "contribute/overview",
                {
                    "group": "Backend",
                    "pages": [
                        "contribute/backend/overview",
                        "contribute/backend/cli"
                    ]
                }
            ]
        }
    ],
    "footerSocials": {
        "x": "https://x.com/opennem",
        "github": "https://github.com/opennem/opennem"
    }
}
